<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal" android:layout_width="fill_parent"
	android:layout_height="wrap_content" android:layout_weight="1">

	<ImageView android:layout_width="48dip" android:id="@+id/image"
		android:layout_height="48dip"></ImageView>

	<LinearLayout android:id="@+id/LinearLayout01"
		android:orientation="vertical" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:layout_weight="1">
		<TextView android:layout_width="fill_parent" android:id="@+id/name"
			android:text="NAME" android:layout_height="fill_parent"
			android:layout_weight="1" android:textAppearance="?android:attr/textAppearanceMedium"></TextView>

		<TextView android:text="TextView01" android:id="@+id/labels"
			android:layout_weight="1" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceSmall"></TextView>
	</LinearLayout>

	<CheckBox style="?android:attr/starStyle" android:id="@+id/starCheck" android:focusable="false"
		android:layout_width="wrap_content" android:layout_marginRight="5dip"
		android:layout_height="wrap_content" android:layout_gravity="center_vertical"  />
</LinearLayout>
